{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "origin-ui",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "origin-ui": {
      "root": "apps/origin-ui",
      "sourceRoot": "apps/origin-ui/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/origin-ui",
            "index": "apps/origin-ui/src/index.html",
            "main": "apps/origin-ui/src/main.tsx",
            "polyfills": "apps/origin-ui/src/polyfills.ts",
            "tsConfig": "apps/origin-ui/tsconfig.app.json",
            "assets": [
              "apps/origin-ui/src/favicon.ico",
              "apps/origin-ui/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/origin-ui/src/environments/environment.ts",
                  "with": "apps/origin-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "origin-ui:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "origin-ui:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/origin-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/origin-ui"],
          "options": {
            "jestConfig": "apps/origin-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "origin-ui-e2e": {
      "root": "apps/origin-ui-e2e",
      "sourceRoot": "apps/origin-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/origin-ui-e2e/cypress.json",
            "tsConfig": "apps/origin-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "origin-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "origin-ui:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/origin-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui-core": {
      "root": "libs/ui/core",
      "sourceRoot": "libs/ui/core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/ui/core",
            "tsConfig": "libs/ui/core/tsconfig.lib.json",
            "project": "libs/ui/core/package.json",
            "entryFile": "libs/ui/core/src/index.ts",
            "external": ["react", "react-dom", "react-is", "@emotion/styled"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/core"],
          "options": {
            "jestConfig": "libs/ui/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui/core/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-core",
            "config": {
              "configFolder": "libs/ui/core/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
