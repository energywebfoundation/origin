    "build:user-ui": "nx run user-data:build --buildableProjectDepsInPackageJsonType=dependencies && nx run user-logic:build --buildableProjectDepsInPackageJsonType=dependencies && nx run user-view:build --buildableProjectDepsInPackageJsonType=dependencies",
    "build:organization-ui": "nx run organization-data:build --buildableProjectDepsInPackageJsonType=dependencies && nx run organization-logic:build --buildableProjectDepsInPackageJsonType=dependencies && nx run organization-view:build --buildableProjectDepsInPackageJsonType=dependencies",
