    "config-env": "bash env.sh",
    "build:ui-utils": "nx run ui-utils:build --buildableProjectDepsInPackageJsonType=dependencies",
    "build:ui-theme": "nx run ui-theme:build --buildableProjectDepsInPackageJsonType=dependencies",
    "build:ui-assets": "nx run ui-assets:build --buildableProjectDepsInPackageJsonType=dependencies",
