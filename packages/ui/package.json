    "build:ui-core": "nx run ui-core:build --buildableProjectDepsInPackageJsonType=dependencies",
    "build:ui-web3": "nx run ui-web3:build --buildableProjectDepsInPackageJsonType=dependencies",
    "build:user-ui": "nx run user-data:build && nx run user-logic:build && nx run user-view:build",
    "build:organization-ui": "nx run organization-data:build && nx run organization-logic:build && nx run organization-view:build",
    "build:certificate-ui": "nx run certificate-data:build && nx run certificate-logic:build && nx run certificate-view:build",
    "build:device-ui": "nx run device-data:build && nx run device-logic:build && nx run device-view:build",
    "build:exchange-ui": "nx run exchange-data:build && nx run exchange-logic:build && nx run exchange-view:build",
    "build:all-packages": "yarn build:ui-utils && yarn build:ui-localization && yarn build:ui-shared-state && yarn build:ui-theme && yarn build:ui-assets && yarn build:ui-core && yarn build:ui-web3 && yarn build:user-ui && yarn build:organization-ui && yarn build:certificate-ui && yarn build:device-ui && yarn build:exchange-ui",
    "build:origin-ui:prod": "cp apps/origin-ui/src/index.html.template apps/origin-ui/src/index.html && nx build --prod",
    "build:origin-ui": "cp apps/origin-ui/src/index.html.template apps/origin-ui/src/index.html && nx build",
    "build:prod": "yarn config-env && yarn build:origin-ui:prod",
    "build": "yarn config-env && yarn build:origin-ui",
    "build:container:canary": "make build-canary",
    "build:container:latest": "make build-latest",
    "deploy:container:heroku:canary": "make push-heroku-canary",
    "deploy:container:heroku:stable": "make push-heroku-stable",
    "storybook": "nx run ui-core:storybook --port=6006",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "./dev-env.sh && nx e2e origin-ui-e2e --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "bundle-report": "webpack-bundle-analyzer dist/apps/origin-ui/stats.json"
