    "build:origin-ui": "cp apps/origin-ui/src/index.html.template apps/origin-ui/src/index.html && nx build",
    "build": "yarn config-env && yarn build:origin-ui",
    "build:container:canary": "make build-canary",
    "build:container:canary-v2": "make build-canary-v2",
    "build:container:latest": "make build-latest",
