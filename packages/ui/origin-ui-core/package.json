{
    "name": "@energyweb/origin-ui-core",
    "version": "1.13.0",
    "main": "dist/src/index.js",
    "scripts": {
        "app:build": "yarn config-env && node --max-old-space-size=8192 ./node_modules/.bin/webpack --config webpack/prod.config.js",
        "build": "yarn build:static && yarn build:ts",
        "build:static": "mkdir -p ./dist/src/styles/ && mkdir -p ./dist/assets/ && cp -r ./src/styles/*.scss ./dist/src/styles/ && cp -r ./assets/* ./dist/assets/",
        "build:ts": "tsc -b tsconfig.json",
        "config-env": "bash env.sh",
        "deploy-storybook": "storybook-to-ghpages",
        "start": "tsc --preserveWatchOutput --watch",
        "start-all": "yarn start",
        "storybook": "start-storybook",
        "lint": "eslint \"src/**/*{.ts,.tsx}\" --quiet",
        "lint-fix": "eslint \"src/**/*{.ts,.tsx}\" --fix",
        "precommit": "lint-staged",
        "prettier": "prettier --write --config-precedence file-override './src/**/*'",
        "clean": "shx rm -rf dist",
        "test": "exit 0 && yarn config-env && jest",
        "test:unit": "yarn config-env && jest -c jest.config.unit.js",
        "test:watch": "yarn config-env && jest -u --watchAll",
        "test:unit:watch": "yarn config-env && jest -u --watchAll -c jest.config.unit.js"
    },
    "dependencies": {
        "@energyweb/device-registry": "8.0.0",
        "@energyweb/exchange-client": "0.1.2",
        "@energyweb/exchange-irec-client": "0.0.2",
        "@energyweb/issuer": "3.1.0",
        "@energyweb/issuer-api-client": "0.1.3",
        "@energyweb/localization": "0.13.0",
        "@energyweb/origin-backend-client": "10.0.2",
        "@energyweb/origin-backend-core": "8.0.0",
        "@energyweb/origin-organization-irec-api-client": "0.1.2",
        "@energyweb/origin-device-registry-irec-form-api-client": "1.0.1",
        "@energyweb/utils-general": "11.0.1",
        "react": "17.0.1",
        "redux": "4.0.5",
        "react-redux": "7.2.2",
        "redux-saga": "1.1.3",
        "react-dom": "17.0.1",
        "react-router-dom": "5.2.0",
        "react-i18next": "11.8.8",
        "react-dropzone": "11.3.1",
        "react-chartjs-2": "2.11.1",
        "@react-google-maps/api": "2.1.1",
        "connected-react-router": "6.9.1",
        "axios": "0.21.1",
        "history": "4.10.1",
        "i18next": "19.9.0",
        "@material-ui/core": "4.11.3",
        "@material-ui/icons": "4.11.2",
        "@material-ui/lab": "4.0.0-alpha.57",
        "@material-ui/pickers": "3.2.10",
        "@material-ui/styles": "^4.10.0",
        "formik": "2.2.6",
        "formik-material-ui": "3.0.1",
        "formik-material-ui-pickers": "0.0.12",
        "yup": "0.32.9",
        "chart.js": "2.9.4",
        "clsx": "1.1.1",
        "ethers": "5.0.31",
        "moment": "2.29.1",
        "moment-timezone": "0.5.33",
        "@date-io/moment": "1.3.13",
        "query-string": "6.14.1",
        "toastr": "2.1.4",
        "winston": "3.3.3",
        "@svgr/webpack": "^5.5.0"
    },
    "devDependencies": {
        "@types/enzyme": "3.10.8",
        "@types/react": "17.0.2",
        "@types/react-redux": "7.1.16",
        "@types/react-router-dom": "5.1.7",
        "@types/redux-logger": "3.0.8",
        "@types/yup": "0.29.11",
        "@testing-library/react": "11.2.5",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "eslint-plugin-react": "7.22.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "jest": "26.6.3",
        "jest-environment-jsdom-fifteen": "1.0.2",
        "redux-logger": "3.0.6",
        "ts-jest": "26.5.2",
        "@storybook/react": "6.1.20",
        "@storybook/storybook-deployer": "2.8.7",
        "@storybook/addon-knobs": "6.1.20"
