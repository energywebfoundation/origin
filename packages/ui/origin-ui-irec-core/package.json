{
    "name": "@energyweb/origin-ui-irec-core",
    "version": "0.0.2",
    "main": "dist/index.js",
    "scripts": {
        "build": "yarn build:static && yarn build:ts",
        "build:static": "mkdir -p ./dist/styles/ && mkdir -p ./dist/assets/ && cp -r ./src/styles/*.scss ./dist/styles/ && cp -r ./assets/* ./dist/assets/",
        "build:ts": "tsc -b tsconfig.json",
        "start": "tsc --preserveWatchOutput --watch",
        "start-all": "yarn start",
        "lint": "eslint \"src/**/*{.ts,.tsx}\" --quiet",
        "lint-fix": "eslint \"src/**/*{.ts,.tsx}\" --fix",
        "precommit": "lint-staged",
        "prettier": "prettier --write --config-precedence file-override './src/**/*'",
        "clean": "shx rm -rf dist"
    },
    "dependencies": {
        "@energyweb/utils-general": "11.0.1",
        "@energyweb/origin-ui-core": "1.13.0",
        "@energyweb/origin-backend-core": "8.0.0",
        "@energyweb/origin-device-registry-api-client": "0.0.2",
        "@energyweb/origin-device-registry-irec-local-api-client": "0.0.2",
        "@energyweb/issuer-api-client": "0.1.3",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-router-dom": "5.2.0",
        "connected-react-router": "6.9.1",
        "redux": "4.0.5",
        "react-redux": "7.2.2",
        "redux-saga": "1.1.3",
        "react-i18next": "11.8.8",
        "@react-google-maps/api": "2.1.1",
        "@material-ui/core": "4.11.3",
        "@material-ui/icons": "4.11.2",
        "@material-ui/lab": "4.0.0-alpha.57",
        "@material-ui/pickers": "3.2.10",
        "@material-ui/styles": "4.11.3",
        "moment": "2.29.1",
        "history": "4.10.1",
        "formik": "2.2.6",
        "formik-material-ui": "3.0.1",
        "yup": "0.32.9",
        "ethers": "5.0.31",
        "axios": "0.21.1",
        "@date-io/moment": "2.10.6",
        "i18next": "19.9.1"
    },
    "devDependencies": {
        "typescript": "4.1.5",
        "@types/react": "17.0.2",
        "@types/react-redux": "7.1.16",
        "@types/react-router-dom": "5.1.7",
        "@types/yup": "0.29.11",
        "eslint-plugin-react": "7.22.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "@types/mocha": "^8.2.1",
        "@types/node": "14.14.31",
        "mocha": "8.3.0",
        "jest": "26.6.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/energywebfoundation/origin.git"
    },
