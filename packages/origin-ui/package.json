  "scripts": {
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/webpack --config webpack/prod.config.js",
    "build:ts": "tsc -b tsconfig.build.json --verbose --pretty",
    "build:full": "webpack --config webpack/prod.config.js",
    "build:container:canary": "make build-canary push",
    "build:container:latest": "make build-latest push",
    "lint": "eslint 'src/**/*{.ts,.tsx}'",
    "lint-fix": "eslint 'src/**/*{.ts,.tsx}' --fix",
    "start": "cross-env NODE_OPTIONS=\"--max_old_space_size=8192\" webpack-dev-server --config webpack/dev.config.js --watch",
    "start-all": "yarn start",
    "test": "shx rm -rf db.sqlite && jest",
    "test:watch": "jest -u --watchAll",
    "precommit": "lint-staged",
    "prettier": "prettier --write --config-precedence file-override './src/**/*'",
    "clean": "shx rm -rf db.sqlite dist"
  },
