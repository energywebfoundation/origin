{
    "name": "@energyweb/origin-backend-app",
    "version": "1.4.0",
    "description": "This is a bootstrap project for Origin API",
    "main": "dist/js/src/index.js",
    "scripts": {
        "start": "nest start -p tsconfig.json",
        "start:dev": "nest start --watch -p tsconfig.json",
        "start:debug": "nest start --debug --watch -p tsconfig.json",
        "start:prod": "node dist/main",
        "prebuild": "shx rm -rf dist",
        "build": "yarn build:ts",
        "build:ts": "tsc -b tsconfig.json",
        "build:container:canary": "make build-canary push",
        "build:container:latest": "make build-latest push",
        "prettier": "prettier --write --config-precedence file-override './src/**/*'",
        "clean": "shx rm -rf dist uploads",
        "lint": "eslint \"src/**/*{.ts,.tsx}\"",
        "lint-fix": "eslint \"src/**/*{.ts,.tsx}\" --fix",
        "precommit": "lint-staged",
        "test:mocha": "mocha -r ts-node/register test/*.e2e-spec.ts --timeout 60000 --exit",
        "test:e2e": "yarn test:mocha"
    },
    "bin": {
        "origin-backend-app": "./bin/origin-backend-app"
    },
    "homepage": "https://github.com/energywebfoundation/origin/tree/master/packages/origin-backend-app#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/energywebfoundation/origin.git"
    },
    "bugs": {
        "url": "https://github.com/energywebfoundation/origin/issues"
    },
    "author": "EnergyWeb DevHub GmbH; Joseph Bagaric, joseph.bagaric@energyweb.org; Piotr Kosinski, piotr.kosinski@energyweb.org",
    "license": "GPL-3.0-or-later",
    "dependencies": {
        "@energyweb/exchange": "1.7.1",
        "@energyweb/exchange-io-erc1888": "1.0.0",
        "@energyweb/exchange-irec": "1.0.0",
        "@energyweb/issuer-api": "0.1.2",
        "@energyweb/migrations": "3.2.2",
        "@energyweb/origin-backend": "9.0.2",
        "@energyweb/origin-backend-core": "7.0.1",
        "@energyweb/origin-backend-utils": "1.4.0",
        "@energyweb/origin-organization-irec-api": "1.2.1",
        "@energyweb/utils-general": "11.0.0",
        "@nestjs-modules/mailer": "1.4.2",
        "@nestjs/common": "7.6.3",
        "@nestjs/config": "^0.6.0",
        "@nestjs/core": "7.6.3",
        "@nestjs/cqrs": "7.0.1",
        "@nestjs/swagger": "4.7.8",
        "@nestjs/typeorm": "7.1.4",
        "body-parser": "1.19.0",
        "class-validator": "0.12.2",
        "cors": "2.8.5",
        "mandrill-nodemailer-transport": "1.2.1",
        "swagger-ui-express": "4.1.4",
        "typeorm": "0.2.29"
    },
    "devDependencies": {
        "@nestjs/testing": "7.6.3",
        "ganache-core": "2.13.1",
        "supertest": "6.0.1"
    },
    "files": [
        "dist",
        "bin"
    ],
    "gitHead": "54beaf7fe6686810de74ca290daf99cbde510f9d",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
