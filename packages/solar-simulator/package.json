{
  "name": "@energyweb/solar-simulator",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start-simulation": "node dist/js/src/simulatorService.js",
    "start-consuming-api": "node dist/js/src/consumerService.js",
    "import-irec-assets": "node dist/js/src/importIRECAssets.js",
    "fund-assets-smart-meters": "node dist/js/src/fundAssets.js",
    "build-ts": "tsc -b .",
    "build": "scripts/build.js",
    "prettier": "prettier --write --config-precedence file-override './src/**/*'",
    "lint": "./node_modules/.bin/tslint 'src/**/*{.ts,.tsx}'",
    "lint-fix": "./node_modules/.bin/tslint --fix 'src/**/*{.ts,.tsx}'",
    "clean": "rm -rf dist"
  },
  "author": "",
  "license": "GPLv3",
  "dependencies": {
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "commander": "3.0.0",
    "cors": "2.8.5",
    "csv": "5.1.1",
    "@energyweb/asset-registry": "1.1.0",
    "@energyweb/utils-general": "1.1.0",
    "express": "4.17.1",
    "fs-extra": "8.0.1",
    "ganache-cli": "6.5.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26",
    "web3": "1.0.0-beta.37",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/web3": "1.0.14",
    "lint-staged": "9.2.1"
  },
  "postinstall": "rm -f node_modules/web3/index.d.ts",
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  },
  "private": true
}
