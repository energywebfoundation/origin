        "@types/mocha": "9.0.0",
        "@types/multer": "1.4.7",
        "@types/node": "14.18.10",
        "@types/nodemailer": "6.4.4",
        "@types/passport": "1.0.7",
        "@types/passport-jwt": "3.0.6",
        "@types/passport-local": "1.0.34",
        "@types/superagent": "4.1.15",
        "@types/supertest": "2.0.11",
