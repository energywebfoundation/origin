        "@types/passport": "1.0.3",
        "@types/passport-jwt": "3.0.3",
        "@types/passport-local": "1.0.33",
        "@types/supertest": "2.0.8",
        "@types/uuid": "7.0.2",
        "@types/websocket": "1.0.0",
        "@types/ws": "7.2.4",
        "axios": "0.19.2",
        "jest": "25.3.0",
        "nodemailer": "6.4.6",
        "supertest": "4.0.2",
