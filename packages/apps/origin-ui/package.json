        "style-loader": "2.0.0",
        "ts-jest": "26.5.4",
        "ts-loader": "8.1.0",
        "url-loader": "4.1.1",
        "webpack": "5.30.0",
        "webpack-cli": "4.6.0",
        "webpack-dev-server": "3.11.2",
        "webpack-merge": "5.7.3",
        "@types/redux-logger": "3.0.8"
    }
