{
    "private": true,
    "name": "@energyweb/origin-ui",
    "version": "2.5.0",
    "scripts": {
        "build": "yarn build:full",
        "build:ts": "tsc -b tsconfig.json",
        "build:full": "yarn config-env && node --max-old-space-size=8192 ./node_modules/webpack/bin/webpack.js --config webpack/prod.config.js",
        "build:container:canary": "make build-canary",
        "build:container:latest": "make build-latest",
        "deploy:container:heroku:canary": "make push-heroku-canary",
        "deploy:container:heroku:stable": "make push-heroku-stable",
        "cypress:open": "./node_modules/.bin/cypress open",
        "config-env": "bash env.sh",
        "start": "yarn config-env && cross-env NODE_OPTIONS=\"--max_old_space_size=8192\" webpack serve --config webpack/dev.config.js",
        "start-all": "yarn start",
        "lint": "eslint \"src/**/*{.ts,.tsx}\" --quiet",
        "lint-fix": "eslint \"src/**/*{.ts,.tsx}\" --fix",
        "precommit": "lint-staged",
        "prettier": "prettier --write --config-precedence file-override './src/**/*'",
        "clean": "shx rm -rf dist"
    },
    "dependencies": {
        "@date-io/moment": "1.3.13",
        "@energyweb/exchange-ui-core": "1.4.0",
        "@energyweb/localization": "0.14.0",
