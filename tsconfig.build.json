{
  "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": true,
      "declaration": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es2018",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "lib": [
          "es2018",
          "dom"
      ],
      "skipLibCheck": true,
      "preserveSymlinks": true
  }
}